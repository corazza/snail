def print(x: A) -> unit as
    __print x;
    return UNIT;
enddef

def println(x: A) -> unit as
    __print x;
    __print __newline;
    return UNIT;
enddef

def input_int() -> int as
    let x: int = 0;
    __input_int x;
    return x;
enddef

def input_string() -> string as
