data Pair<C, D> as
    Pa(C, D)
enddata

def print_both<A, B>(a: A, b: B) -> unit as
    print "(";
    print a;
    print ", ";
    print b;
    print ")";
    return UNIT;
enddef

def print_pair<A, B>(p: Pair<A, B>) -> unit as
    match p as
        Pa(a, b) => print_both(a, b)
    endmatch
    return UNIT;
enddef

// let p3: Pair<int, Pair<string, int>> = Pa(1, Pa(4, 3)); // TYPE ERROR!
let p3: Pair<int, Pair<string, int>> = Pa(1, Pa("asdf", 3)); // OK

print "p3 = ";
print_pair(p3);
