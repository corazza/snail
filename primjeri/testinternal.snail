data Pair<C, D> as
    Pa(C, D)
enddata

def print(x: A) -> unit as
    __print x;
    return UNIT;
enddef

def println(x: A) -> unit as
    __print x;
    __print __newline;
    return UNIT;
enddef

def treba_iste(p: Pair<A, A>) -> A as
    match p as
        Pa(a, b) => return a
    endmatch
enddef

let p: Pair<int, string> = Pa(1, "asdf");


print(treba_iste(p));
